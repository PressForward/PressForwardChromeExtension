{
	"name": "PressForward Nominate This",
	"version": "0.1",
	"description": "Nominate This",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "icon.png"
	},
	"options_ui": {
		"page": "settings.html",
		"open_in_tab": false
	},
	"web_accessible_resources": [
		"nomthis.js",
		"pell.js",
		"submit.js",
		"nomthis.css.js"
	],
	"permissions": [
		"storage",
		"declarativeContent",
		"activeTab",
		"clipboardRead",
		"background",
		"http://*/*",
		"https://*/*"
	],
	"background": {
		"scripts": [
			"background.js",
			"handler.js",
			"nomthis-init.js"
		]
	},
	"content_security_policy": "script-src 'self' https:* 'unsafe-inline'; object-src 'self'"
}